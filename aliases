
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

alias p="cd ~/projects"

alias mkdir="mkdir -p"

alias ls='ls --color=auto'
alias ll='ls -alF'
alias pll='pwd && ll'

alias evi='vim ~/.vimrc'

alias grep='grep --color=auto'

alias dk='docker'
alias dkps='docker ps -a'
alias dkr='docker run'
alias dki='docker images'
alias dks='docker service'

alias tf='terraform's

